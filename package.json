{
  "name": "melbpt-utils",
  "version": "0.1.19",
  "description": "A library for code common to the various melbpt services.",
  "main": "index.js",
  "types": "index.d.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./index.js"
    }
  },
  "scripts": {
    "clean": "rimraf dist/*",
    "prebuild": "npm run clean",
    "build": "tsc --project ts/tsconfig.json",
    "lint": "eslint . & tsc --project ts/tsconfig.json --noemit & tsc --project tests/tsconfig.json --noemit",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schel-d/melbpt-utils.git"
  },
  "author": "schel-d",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schel-d/melbpt-utils/issues"
  },
  "homepage": "https://github.com/schel-d/melbpt-utils#readme",
  "keywords": [
    "melbpt",
    "trainquery",
    ".ttbl"
  ],
  "devDependencies": {
    "@types/jest": "^29.2.1",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "esbuild": "^0.15.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^29.2.2",
    "npm-dts": "^1.3.12",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "dist/**/*"
  ],
  "dependencies": {
    "@types/luxon": "^3.0.2",
    "luxon": "^3.0.4",
    "zod": "^3.19.1"
  }
}
