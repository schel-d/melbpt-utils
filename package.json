{
  "name": "melbpt-utils",
  "version": "0.1.4",
  "description": "A library for code common to my various melbpt projects.",
  "main": ".out/index.js",
  "types": ".out/index.d.js",
  "type": "module",
  "scripts": {
    "clean": "rimraf .out/*",
    "prebuild": "npm run clean",
    "build": "tsc --project ts/tsconfig.json",
    "lint": "eslint . & tsc --project ts/tsconfig.json --noemit & tsc --project tests/tsconfig.json --noemit",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schel-d/melbpt-utils.git"
  },
  "author": "schel-d",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/schel-d/melbpt-utils/issues"
  },
  "homepage": "https://github.com/schel-d/melbpt-utils#readme",
  "keywords": [
    "melbpt",
    "trainquery",
    ".ttbl"
  ],
  "devDependencies": {
    "@types/luxon": "^3.0.2",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  },
  "files": [
    "index.js",
    "index.d.ts",
    ".out/**/*"
  ],
  "dependencies": {
    "@types/jest": "^29.2.0",
    "jest": "^29.2.1",
    "luxon": "^3.0.4",
    "ts-jest": "^29.0.3",
    "zod": "^3.19.1"
  }
}
